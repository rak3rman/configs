{
  "keymap": [
    {
      "key": "ctrl-z",
      "trigger": "jump-to-terminal"
    },
    {
      "key": "ctrl-cmd-right",
      "trigger": "next-tab"
    },
    {
      "key": "ctrl-cmd-left",
      "trigger": "prev-tab"
    },
    {
      "key": "cmd-period cmd-d",
      "trigger": "ai/generate-documentation"
    },
    {
      "key": "cmd-shift-semicolon",
      "trigger": "-ai/toggle-inline-ai-prompt"
    },
    {
      "key": "cmd-period",
      "trigger": "-ai/toggle-inline-ai-prompt"
    },
    {
      "key": "cmd-period cmd-g",
      "trigger": "ai/toggle-inline-ai-prompt"
    },
    {
      "key": "cmd-period cmd-e",
      "trigger": "ai/explain-selected-code"
    },
    {
      "key": "ctrl-a",
      "trigger": "ai/jump-to-assistant-tool"
    },
    {
      "key": "ctrl-cmd-a",
      "trigger": "ai/jump-to-assistant-tool"
    },
    {
      "key": "ctrl-cmd-z",
      "trigger": "jump-to-terminal"
    },
    {
      "key": "cmd-capslock",
      "trigger": "show-tooltip"
    },
    {
      "key": "ctrl-return",
      "trigger": "show-tooltip"
    }
  ]
}